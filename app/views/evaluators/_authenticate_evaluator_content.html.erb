<div class="row">
  <div class="col-sm-2">
    <%= render 'static/menu_publicaciones' %>
  </div>
  <div class="text-center">
    <div class="col-md-10" style="padding: 10px 0px 0px 0px;">
      <h1 style="font-size:200%;">Bienvenido evaluador</h1>
    </div>
  </div>

  <div class="col-sm-10">
    <div class="main-content">
      <div class="form-basic">
        <div class="text-center">
          <div class="login-form" style="height:350px">
            <%= form_for(:evaluation, :url => evaluate_publication_path) do |f| %>
            <div class="text-center">
              <h4>Iniciar sesi√≥n de Evaluador</h4>
              <div><hr class="style13"></div>
            </div>
            <div class="sign-in-htm">
              <div class="group">
                <label for="user" class="label">Correo</label>
                <%= f.text_field :email, :id=>"email", :type=>"text", :class=>"input", autofocus: true %>
              </div>

              <div class="group">
                <label for="user" class="label">Codigo</label>
                <%= f.password_field :code, :id=>"pass", :type=>"password", :class=>"input", autocomplete: "off" %>
              </div>

              <div class="field">
                <%= f.hidden_field :url_token,  :value => @url_token %>
              </div>
              <div><hr class="style13"></div>
              <div class="group">
                <div class="actions">
                  <%= f.submit "Autenticar", :class=>"button"%>
                </div>
              </div>
              <% end %>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div><hr class="style13"></div>
    <div class="col-sm-12">
      <div class="form-basic-2">
        <div class="text-center">
          <div class="li-custom" style="width:100px; margin:auto; padding:10px 10px 10px 10px">
            <%= link_to 'Volver', administrator_home_path %>
          </div>
        </div>
      </div>
    </div>
