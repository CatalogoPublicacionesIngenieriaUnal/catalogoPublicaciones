<div class="row">
  <div class="col-sm-2">
    <%= render 'static/menu_publicaciones' %>
  </div>
  <div class="col-sm-10">
    <div class="text-center" style="padding: 10px 0px 0px 0px">
      <h1 style="font-size:200%;">Evaluadores</h1>
      <p id="notice"><%= notice %></p>
    </div>
    <div><hr class="style13"></div>
    <div class="form-basic-2" style="width:920px">
      <%= will_paginate @publications, :style => "background:none"%>
      <table class="table-custom">
        <tr>
          <th>Nombres</th>
          <th>Apellidos</th>
          <th>Correo electr√≥nico</th>
          <th>Idioma</th>
          <th colspan="3"></th>
        </tr>
        <tbody>
          <% @evaluators.each do |evaluator| %>
          <tr>
            <td><%= evaluator.first_name %></td>
            <td><%= evaluator.last_name %></td>
            <td><%= evaluator.email %></td>
            <td><%= evaluator.language_id %></td>
            <td><%= link_to 'Show', evaluator %></td>
            <td><%= link_to 'Edit', edit_evaluator_path(evaluator) %></td>
            <td><%= link_to 'Destroy', evaluator, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
    <div><hr class="style13"></div>
    <div class="form-basic-2">
      <div class="text-center">
        <div class="li-custom" style="width:100px; margin:auto; padding:10px 10px 10px 10px">
          <%= link_to 'Volver', :back %>
        </div>
      </div>
    </div>
  </div>
</div>
