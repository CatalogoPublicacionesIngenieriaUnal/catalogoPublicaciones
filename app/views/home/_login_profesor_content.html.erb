<div class="login-form" style="height:350px">
  <%= form_for(resource, :as => resource_name, :url => new_session_path(resource_name)) do |f| %>
  <div class="text-center">
    <h4>Iniciar sesión de Profesor</h4>
    <div><hr class="style13"></div>
  </div>
  <div class="sign-in-htm">
    <div class="group">
      <label for="user" class="label">Usuario</label>
      <%= f.text_field :username, :id=>"professor_username", :type=>"text", :class=>"input",autofocus: true %>
    </div>
    <div class="group">
      <label for="pass" class="label">Contraseña</label>
      <%= f.password_field :password,:id=>"professor_password", :type=>"password", :class=>"input", autocomplete: "off" %>
    </div>
    <div class="group" style="padding: 39px 0px 0px 0px">
      <div class="actions">
        <%= f.submit "Ingresar", :class=>"button"%>
      </div>
      <% end %>
    </div>
  </div>
</div>
