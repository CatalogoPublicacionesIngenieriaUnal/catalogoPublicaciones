  </div>

  <div class="field">
    <%= f.label :created_at %>
    <%= f.date_select :created_at %>
  </div>

  <div class="field">
    <%= f.label :finish_at %>
    <%= f.date_select :finish_at %>
  </div>

  <div class="field">
    <%= f.label :telephone %>
