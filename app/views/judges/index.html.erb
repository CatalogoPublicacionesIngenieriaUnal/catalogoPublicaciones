<p id="notice"><%= notice %></p>

<h1>Judges</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Name</th>
      <th>Lastname</th>
      <th>Password</th>
      <th>Email</th>
      <th>Created at</th>
      <th>Finish at</th>
      <th>Telephone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @judges.each do |judge| %>
      <tr>
        <td><%= judge.user %></td>
        <td><%= judge.name %></td>
        <td><%= judge.lastname %></td>
        <td><%= judge.password %></td>
        <td><%= judge.email %></td>
        <td><%= judge.created_at %></td>
        <td><%= judge.finish_at %></td>
        <td><%= judge.telephone %></td>
        <td><%= link_to 'Show', judge %></td>
        <td><%= link_to 'Edit', edit_judge_path(judge) %></td>
        <td><%= link_to 'Destroy', judge, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Judge', new_judge_path %>
