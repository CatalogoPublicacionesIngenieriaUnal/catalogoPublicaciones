<div class="row">
  <div class="col-sm-2">
    <%= render 'static/menu_publicaciones' %>
  </div>
  <div class="col-sm-10">
    <div class="text-center" style="padding: 10px 0px 0px 0px">
      <h1 style="font-size:200%;">Estad√≠sticas</h1>
      <p id="notice"><%= notice %></p>
    </div>
    <div><hr class="style13"></div>
    <%= javascript_include_tag "graphs" %>
    <center>
      <button class="button1 button2" onclick="categ()">Categoria</button>
      <button class="button1 button2" onclick="them()">Tema</button>
      <br><br>
      <svg id="graficas"></svg>
    </center>

    <div><hr class="style13"></div>
    <% if professor_signed_in? %>
    <div class="col-sm-12">
      <div class="form-basic-2">
        <div class="text-center">
          <div class="li-custom" style="width:100px; margin:auto; padding:10px 10px 10px 10px">
            <%= link_to 'Volver', professor_home_path %>
          </div>
        </div>
      </div>
    </div>
    <% elsif administrator_signed_in? %>
    <div class="col-sm-12">
      <div class="form-basic-2">
        <div class="text-center">
          <div class="li-custom" style="width:100px; margin:auto; padding:10px 10px 10px 10px">
            <%= link_to 'Volver', :back %>
          </div>
        </div>
      </div>
    </div>
    <% end %>
  </div>
</div>
